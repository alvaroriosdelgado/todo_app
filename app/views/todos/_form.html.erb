<div class="container">
  <div class="jumbotron">
    <%= form_with model: @todo, local: true do |f| %>
    <div class="form-group">
      <%= f.label :description, "Descripcion" %>
      <%= f.text_area :description, class:"form-control", rows:"3" %>
    </div>
      <%= f.submit "Guardar", class:"btn btn-success" %>
    <% end %>
  </div>
</div>
